      "type": "image/png",
      "purpose": "maskable"
    },
    {
      "src": "https://placehold.co/512x512/5B2EFF/ffffff.png?text=PV",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "any"
    },
    {
      "src": "https://placehold.co/512x512/5B2EFF/ffffff.png?text=PV",
      "sizes": "512x512",
      "type": "image/png",
      "purpose": "maskable"
    }
  ],
  "screenshots": [
    {
      "src": "https://placehold.co/1080x1920/111827/5B2EFF.png?text=PYQverse+Home",
      "sizes": "1080x1920",
      "type": "image/png",
      "form_factor": "narrow",
      "label": "Smart Dashboard"
    },
    {
      "src": "https://placehold.co/1080x1920/ffffff/5B2EFF.png?text=AI+Practice",
      "sizes": "1080x1920",
      "type": "image/png",
      "form_factor": "narrow",
      "label": "AI Practice Questions"
    },
    {
      "src": "https://placehold.co/1920x1080/111827/5B2EFF.png?text=Desktop+Experience",
      "sizes": "1920x1080",
      "type": "image/png",
      "form_factor": "wide",
      "label": "Desktop Experience"
    }
  ],
  "shortcuts": [
    {
      "name": "Start Practice",
      "short_name": "Practice",
      "description": "Start a new practice session",
      "url": "/?action=practice",
      "icons": [{ "src": "https://placehold.co/192x192/5B2EFF/ffffff.png?text=P", "sizes": "192x192", "type": "image/png" }]
    },
    {
      "name": "Current Affairs",
      "short_name": "News",
      "description": "Check daily updates",
      "url": "/?action=news",
      "icons": [{ "src": "https://placehold.co/192x192/10B981/ffffff.png?text=N", "sizes": "192x192", "type": "image/png" }]
    }
  ],
  "share_target": {
    "action": "/?action=share",
    "method": "GET",
    "enctype": "application/x-www-form-urlencoded",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "file_handlers": [
    {
      "action": "/?action=open-file",
      "name": "PYQverse Notes",
      "accept": {
        "text/plain": [".txt", ".note", ".json"]
      },
      "icons": [
        {
          "src": "https://placehold.co/192x192/5B2EFF/ffffff.png?text=File",
          "sizes": "192x192",
          "type": "image/png"
        }
      ],
      "launch_type": "single-client"
    }
  ],
  "protocol_handlers": [
    {
      "protocol": "web+pyqverse",
      "url": "/?action=%s"
    }
  ],
  "widgets": [
    {
      "name": "Question of the Day",
      "short_name": "QOTD",
      "description": "See the daily challenge question",
      "tag": "qotd",
      "template": "widget-template.json",
      "data": "widget-data.json",
      "type": "application/json",
      "auth": false,
      "update": 86400,
      "icons": [
        {
          "src": "https://placehold.co/96x96/5B2EFF/ffffff.png?text=Q",
          "sizes": "96x96",
          "type": "image/png"
        },
        {
          "src": "https://placehold.co/192x192/5B2EFF/ffffff.png?text=Q",
          "sizes": "192x192",
          "type": "image/png"
        }
      ]
    }
  ],
  "note_taking": {
    "new_note_url": "/?action=upload"
  },
  "edge_side_panel": {
    "preferred_width": 400
  },
  "scope_extensions": [
    {
      "origin": "https://pyqverse.vercel.app"
    }
  ],
  "related_applications": [],
  "launch_handler": {
    "client_mode": ["focus-existing", "auto"]
  }
}