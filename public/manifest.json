{
  "name": "PYQverse",
  "short_name": "PYQverse",
  "description": "PYQverse: All exams ka pura universe. AI-powered Previous Year Questions, smart generation, and fast, accurate exam prep for UPSC, SSC, NEET, JEE, and more.",
  "id": "/",
  "start_url": "/?source=pwa",
  "scope": "/",
  "display": "standalone",
  "display_override": ["window-controls-overlay", "standalone", "minimal-ui"],
  "background_color": "#111827",
  "theme_color": "#5B2EFF",
  "orientation": "portrait",
  "categories": ["education", "productivity", "utilities"],
  "lang": "en",
  "dir": "ltr",
  "iarc_rating_id": "",
  "handle_links": "preferred",
  "prefer_related_applications": false,
  "icons": [
