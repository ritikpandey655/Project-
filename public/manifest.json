
{
  "name": "PYQverse AI Exam Prep",
  "short_name": "PYQverse",
  "description": "Master UPSC, SSC, JEE, NEET & UP Board with the world's most accurate AI exam patterns.",
  "id": "pyqverse-app-v11",
  "start_url": "/?source=pwa",
  "scope": "/",
  "display": "standalone",
  "display_override": ["window-controls-overlay", "standalone", "minimal-ui"],
  "orientation": "portrait-primary",
  "theme_color": "#5B2EFF",
