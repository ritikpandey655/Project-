
{
  "name": "PYQverse",
  "short_name": "PYQverse",
  "start_url": "/?source=pwa",
  "id": "/",
  "scope": "/",
  "display": "standalone",
  "display_override": ["window-controls-overlay", "standalone"],
  "background_color": "#111827",
  "theme_color": "#5B2EFF",
  "orientation": "portrait-primary",
  "categories": ["education", "productivity", "utilities"],
  "description": "PYQverse is the ultimate AI-powered exam preparation platform. Practice Previous Year Questions (PYQs), take smart Mock Tests, and stay updated with daily Current Affairs for UPSC, SSC, NEET, JEE, and more. Features offline mode and smart analytics.",
  "lang": "en",
  "dir": "ltr",
  "iarc_rating_id": "e84b072d-71b3-4d3e-86ae-31a8ce02a73d",
  "edge_side_panel": {
    "preferred_width": 400
  },
  "launch_handler": {
    "client_mode": ["focus-existing", "auto"]
  },
  "share_target": {
    "action": "/?action=share",
    "method": "GET",
    "enctype": "application/x-www-form-urlencoded",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url"
    }
  },
  "protocol_handlers": [
    {
      "protocol": "mailto",
      "url": "/?mail=%s"
    },
    {
      "protocol": "web+pyqverse",
      "url": "/?link=%s"
    }
  ],
  "file_handlers": [
    {
      "action": "/?action=open-file",
      "accept": {
        "application/json": [".json"]
      },
      "icons": [
        {
          "src": "https://placehold.co/192x192/5B2EFF/ffffff.png?text=File",
          "sizes": "192x192",
          "type": "image/png"
        }
      ],
      "launch_type": "single-client"
    }
  ],
  "scope_extensions": [
    {
      "origin": "https://pyqverse.vercel.app"
    }
  ],
  "widgets": [
    {
      "name": "Daily Question",
      "short_name": "QOTD",
      "description": "View the Question of the Day",
      "tag": "qotd-widget",
      "template": "qotd-template",
      "ms_ac_template": "widgets/qotd.json",
      "data": "widgets/qotd-data.json",
      "type": "application/json",
      "screenshots": [
        {
          "src": "https://placehold.co/400x200/5B2EFF/ffffff.png?text=Widget",
          "sizes": "400x200",
          "label": "Daily Question Widget"
        }
      ],
      "icons": [
        {
          "src": "https://placehold.co/192x192/5B2EFF/ffffff.png?text=Q",
          "sizes": "192x192",
          "type": "image/png"
        }
      ]
    }
  ],
  "note_taking": {
    "new_note_url": "/?action=new-note"
  },
  "related_applications": [
    {
      "platform": "play",
