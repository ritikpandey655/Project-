
{
  "version": 2,
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.py"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "builds": [
    {
        "src": "api/index.py",
        "use": "@vercel/python"
    },
    {
        "src": "package.json",
        "use": "@vercel/static-build"
    }
  ]
}
